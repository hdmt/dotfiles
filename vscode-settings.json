{
  "editor.fontSize": 16,
  "terminal.integrated.fontSize": 18,
  "workbench.startupEditor": "none",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.copyOnSelection": true,
  "git.ignoreLegacyWarning": true,
  "editor.renderWhitespace": "all",
  "editor.wordWrap": "on",
  "diffEditor.wordWrap": "on",
  "security.workspace.trust.untrustedFiles": "open",
  "[ruby]": {
    "editor.formatOnSave": true
  },
  "editor.copyWithSyntaxHighlighting": false,
  "gitlens.advanced.messages": {
    "suppressGitVersionWarning": true
  },
  "[blade]": {
    "editor.autoClosingBrackets": "always"
  },
  "markdown-pdf.outputDirectory": "/Users/hidemitsutanaka/workspace",
  "markdown-pdf.displayHeaderFooter": false,
  "markdown.preview.fontSize": 16,
  "editor.detectIndentation": false,
  "workbench.colorTheme": "GitHub Dark", 
  "editor.inlineSuggest.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "never",
  "workbench.editor.empty.hint": "hidden",
  "update.showReleaseNotes": false,
  "[prisma]": {
    "editor.formatOnSave": false
  },
  "github.copilot.advanced": {
  
  },
  "window.zoomLevel": 1,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[python]": {
    "diffEditor.ignoreTrimWhitespace": false,
    "gitlens.codeLens.symbolScopes": ["!Module"],
    "editor.formatOnType": true,
    "editor.wordBasedSuggestions": "off"
  },
  "remote.SSH.enableRemoteCommand": true,
  "prisma.showPrismaDataPlatformNotification": false,
  "github.copilot.editor.enableAutoCompletions": true,
  "[terraform]": {
    "editor.defaultFormatter": null,
    "editor.formatOnSave": false
  },
  "chat.editing.alwaysSaveWithGeneratedChanges": true,
  "github.copilot.chat.localeOverride": "ja",
  "editor.minimap.enabled": false,
  "editor.stickyScroll.enabled": false,
  "gitlens.currentLine.dateFormat": "",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "biome.enabled": true,
  "[javascript][typescript][typescriptreact][javascriptreact][css][postcss][json][jsonc]": {
    "editor.defaultFormatter": "biomejs.biome",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "quickfix.biome": "explicit",
      "source.organizeImports.biome": "explicit"
    }
  },
  "git.openRepositoryInParentFolders": "never",
  "cline.preferredLanguage": "Japanese - 日本語",
  "workbench.colorCustomizations": {
    "terminal.background": "#192544",
    "terminal.foreground": "#a7c8e8",
    "terminalCursor.foreground": "#89b4fa",
  },
  "chat.editor.fontSize": 16,
  "chat.agent.enabled": true,
  "chat.agent.maxRequests": 20,



}
